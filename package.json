{
  "name": "webpack-boilerplate",
  "version": "0.1.0",
  "description": "A boilerplate for webpack.",
  "repository": "git@github.com:QuentinRoy/noel-santa.git",
  "author": "Quentin Roy <quentin@quentinroy.fr>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.dev.js",
